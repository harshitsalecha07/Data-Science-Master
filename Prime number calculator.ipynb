   "source": [
    "n=int(input(\"Entre your number : \"))\n",
    "import math\n",
    "a=int(math.sqrt(n))\n",
    "b=a+1\n",
    "x=1\n",
    "for i in range(b):\n",
    "    if i>=2:\n",
    "        if n%i==0:\n",
    "            x=0\n",
    "            break\n",
    "if x==0:\n",
    "    print(\"Your number is not prime\")\n",
    "else:\n",
    "    print(\"Your number is prime\")"
   ]
